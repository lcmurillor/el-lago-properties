{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "propertyName": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "propertyName"
    },
    "description": {
      "type": "text"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "location": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    }
  }
}
